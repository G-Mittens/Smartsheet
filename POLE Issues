+ IF(HAS([ANCHOR (GIGs)]@row, "No GG (Guy-Guard)"), "Install Guy-Guard / ", "")
+ IF(HAS([ANCHOR (GIGs)]@row, "Anchor is pulling out"), "Anchor is pulling out / ", "")
+ IF(HAS([ANCHOR (GIGs)]@row, "Anchor is off lead"), "Correct anchor lead alignment / ", "")
+ IF(HAS([ANCHOR (GIGs)]@row, "Missing grounding from DG to strand"), "Install preform from DG to strand / ", "")
+ IF(HAS([ANCHOR (GIGs)]@row, "DG in the wrong eyelet"), "Move DG to correct eyelet / ", "")
+ IF(HAS([ANCHOR (GIGs)]@row, "DG is touching the electrics DG"), "DG can't be touching electrics / ", "")
+ IF(HAS([MST (GIGs)]@row, "WRONG POLE"), "MST located at the wrong pole (Calls for supervisor review) / ", "") 
